# assign
a = 123 end

a1 = 19.2 end

a2 = a1 end

a3 = 2 * (1 + 3) - (8 / 4) end

5 + 2 end

abc == 123 end

abc > 123 end

abc < 123 end

123 > abc end

abc <= 123 end

abc >= 123 end

123 <= abc end

# if-else statement
if ab == 12 do abc = 1 end

if ab == 12 and bc == 13 do ab = 2 end

if ab == 12 and bc == 13 do
    if a < 1 and b > 10 do
        if x < 10 and y >= 5 do
            u = 1
        end
        else do u = 0 end
    end
    else do y = 0 end
    end
else do z = 0 end

# forLoop
for u = 1 to 10 do
    ab = 1
    b = u + 1
end

for u12 = 1 to 10 do
    for i = 2 to 9 do
        i = i + 1
        v = i
    end
    u12 = u12 + 1
end

# whileLoop
while ab123 < 10 do
    i = ab123
    ab123 = ab123 + 1
    end

while abc123 < 10 and abc1234 >= 0 do
    while ab12 > 100 do
        i = ab123
        ab123 = ab123 + 1
        ab1234 = ab1234 - 1
    end
    break = true
end

# mix
if ab == 12 and bc == 13 do
    if a < 1 and b > 10 do
        if x < 10 and y >= 5 do
            for u12 = 1 to 10 do
                while abc < 10 do
                    i = abc
                    abc = abc + 1
                end
                i = 2
                u12 = u12
            end
        end
        else do x = 6 end
    end
    else do a = 12 end
end
else do a = b end

if ab == 12 and bc == 13 do
    if a < 1 and b > 10 do
        if x < 10 and y >= 5 do
            for u12 = 1 to 10 do
                while abc < 10 do
                    i = abc
                    abc = abc + 1
                end
                i = 2
                u12 = u12
            end
        end
        elif x <= 100 do
            ab = u
        end
        else do x = 6 end
    end
    else do a = 12 end
end
else do a = b end

# elif
if ab == 1 do a = 1 end
elif ab == 2 do a = 2 end
else a = 3 end

if ab == 12 and bc == 13 do
    if a < 1 and b > 10 do
        u = 1
    end
    elif a <= 2 do
        u = abc
    end
    else do u = 0 end
else do z = 0 end

